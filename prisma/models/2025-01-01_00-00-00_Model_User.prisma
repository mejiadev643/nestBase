model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
  password String
  roleId Int
  isActive Boolean @default(true)
  lastLogin DateTime? @default(now())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  role Role @relation(fields: [roleId], references: [id])
}